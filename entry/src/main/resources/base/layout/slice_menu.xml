<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:id="$+id:parent"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">

    <Text
        ohos:id="$+id:header"
        ohos:height="102vp"
        ohos:width="match_parent"
        ohos:background_element="$graphic:menu_bg"
        ohos:left_padding="60vp"
        ohos:max_text_lines="3"
        ohos:multiple_lines="true"
        ohos:right_padding="8vp"
        ohos:text="$string:lorem_ipsum"
        ohos:text_color="$color:white"
        ohos:text_font="sans-serif-light"
        ohos:text_size="22fp"
        ohos:top_padding="8vp"
        />

    <Text
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:below="$+id:header"
        ohos:multiple_lines="true"
        ohos:padding="16vp"
        ohos:text="$string:lorem_ipsum_large"
        ohos:text_size="18fp"
        />

    <com.github.clans.fab.FloatingActionMenu
        ohos:id="$+id:menu_yellow"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:align_parent_bottom="true"
        ohos:align_parent_right="true"
        ohos:bottom_margin="10vp"
        ohos:left_margin="10vp"
        ohos:menu_animationDelayPerItem="0"
        ohos:menu_colorNormal="#FFB805"
        ohos:menu_colorPressed="#F2AB00"
        ohos:right_margin="80vp"
        ohos:top_margin="10vp"
        >

        <com.github.clans.fab.FloatingActionButton
            ohos:id="$+id:fab12"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:fab_colorNormal="#FFB805"
            ohos:fab_colorPressed="#F2AB00"
            ohos:fab_label="Menu item 1"
            ohos:fab_size="1"
            ohos:image_src="$media:ic_edit"/>

        <com.github.clans.fab.FloatingActionButton
            ohos:id="$+id:fab22"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:fab_colorNormal="#FFB805"
            ohos:fab_colorPressed="#F2AB00"
            ohos:fab_label="Menu item 2"
            ohos:fab_size="1"
